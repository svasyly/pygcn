CREATE TABLE `glade_catalog` (
  `glade_id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `ra0` double,
  `dec0` double,
  `dist` double,
  `bmag` double,
  `bt_hyp` double,
  `e_bt_hyp` double,
  `it_hyp` double,
  `e_it_hyp` double,
  `modz_hyp` double,
  `mod0_hyp` double,
  `logd25_hyp` double,
  `e_logd25_hyp` double,
  `logr25_hyp` double,
  `e_logr25_hyp` double,
  `logdc_hyp` double,
  `pa_hyp` double,
  `btc_hyp` double,
  `itc_hyp` double,
  `ubtc_hyp` double,
  `bvtc_hyp` double,
  `jmag_2mass` double,
  `errjmag_2mass` double,
  `hmag_2mass` double,
  `errhmag_2mass` double,
  `kmag_2mass` double,
  `errkmag_2mass` double,
  `ab_ratio_2mass` double,
  `pa_in_kmag_2mass` double,
  `bmag_gwgc` double,
  `majdiam_gwgc` double,
  `errmd_gwgc` double,
  `mindiam_gwgc` double,
  `errmid_gwgc` double,
  `pa_gwgc` double,
  `dist_gwgc` double,
  `errdist_gwgc` double,
  `errbmag_gwgc` double,
  `kmag_2mpz` double,
  `errkmag_2mpz` double,
  `bmag_2mpz` double,
  `errbmag_2mpz` double,
  `errbmag_2_2mpz` double,
  `zspec_2mpz` double,
  `zphot_2mpz` double,
  `errzphot_2mpz` double,
  `errzphot_2_2mpz` double,
  `flag` double,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
